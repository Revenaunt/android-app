<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/header_icon_circle"
    android:layout_width="@dimen/header_content_size"
    android:layout_height="@dimen/header_content_size"
    android:layout_marginBottom="15dp"
    android:gravity="center"
    android:layout_gravity="center"
    android:background="@drawable/circle_white" >

    <ImageView
        android:contentDescription="@string/header_icon_cd"
        android:id="@+id/header_icon_icon"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:padding="40dp"
        android:src="@drawable/ic_compass_white_50dp" />

</RelativeLayout>
