<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/header_icon_circle"
    android:layout_width="@dimen/header_goal_icon_image_size"
    android:layout_height="@dimen/header_goal_icon_image_size"
    android:gravity="center"
    android:layout_gravity="center"
    android:background="@drawable/circle_gray" >

    <ImageView
        android:contentDescription="@string/header_icon_cd"
        android:id="@+id/header_icon_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:padding="10dp"
        android:src="@drawable/ic_action_compass_white" />

</RelativeLayout>
