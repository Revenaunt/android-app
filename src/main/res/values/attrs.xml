<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <attr name="fontPath" format="string"/>

    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color"/>
        <attr name="fab_colorNormal" format="color"/>
        <attr name="fab_colorRipple" format="color"/>
        <attr name="fab_shadow" format="boolean"/>
        <attr name="fab_type" format="enum">
            <enum name="normal" value="0"/>
            <enum name="mini" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SpacingItemDecoration">
        <attr name="android:verticalSpacing" tools:ignore="ResourceName"/>
        <attr name="android:horizontalSpacing" tools:ignore="ResourceName"/>
    </declare-styleable>

</resources>